<%- include('header', title) %>
<p><%= message %></p>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Mail</th>
    </tr>
  </thead>
  <tbody>
    <% for (const item of db) { %>
    <tr>
      <td><%= item.name %></td>
      <td><%= item.mail %></td>
    </tr>
    <% } %>
  </tbody>
</table>
<%-include('footer') %>
